<div class="col-sm-6 col-md-4 col-md-offset-4">
	<h1 class="text-center login-title">VocÃª precisa estar logado para utilizar o sistema</h1>
	<div class="account-wall">
		<img class="profile-img" src="https://lh5.googleusercontent.com/-b0-k99FZlyE/AAAAAAAAAAI/AAAAAAAAAAA/eu7opA4byxI/photo.jpg?sz=120"
		alt="">

		<%= form_for(resource, :as => resource_name, :url => session_path(resource_name), html: { class: "form-signin"}) do |f| %>

		<%= f.label :'E-mail:' %><br />
		<%= f.email_field :email, :autofocus => true, class: "form-control" %>
		<%= f.label :'Senha:' %><br />
		<%= f.password_field :password, class: "form-control" %>

		<%= f.submit "Entrar", class: "btn btn-lg btn-primary btn-block" %>

		<% end %>
	</div>
	<div class="text-center new-account">
		<%= render "devise/shared/links" %>
	</div>
</div>
